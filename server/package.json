{
  "name": "fullstack",
  "version": "1.0.0",
  "description": "fullstack app with angular and nodejs",
  "main": "index.js",
  "engines": {
    "node": "12.13.0",
    "npm": "6.12.0"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js --inspect",
    "client-install": "npm install --prefix client",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "keywords": [
    "nodejs",
    "mongodb"
  ],
  "author": "alexei strug",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.779.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongodb": "^3.6.2",
    "mongoose": "^5.10.10",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nodemon": "^2.0.6",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pm2": "^4.5.0"
  },
  "devDependencies": {}
}
